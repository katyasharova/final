<p id="notice"><%= notice %></p> 
<h1>Users</h1> 
<table id="users_list">
 <thead>
    <tr><th>Login</th>
    <th>Password</th>
     <th>Actions</th> 
     </tr> 
</thead> 
<tbody> 
<% User.all.each do |user| %> 
<tr><td><%= user.login %></td> 
<td><%= '*' * 5 %></td> 
<td><%= link_to 'Destroy', user, method: :delete, data: { confirm: 'Are you sure?' } %></td> 
</tr> 
<% end %> 
</tbody> 
</table> 
<br> <%= link_to 'Back', root_path %>
<br> <%= link_to 'New User', new_user_path %>